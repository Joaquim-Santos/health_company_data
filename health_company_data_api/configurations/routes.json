{
  "blueprint": {
    "name": "api",
    "url_prefix": "/api",
    "resources": [
      {
        "name": "health",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "HealthResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/health",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["GET"]
            }
          }
        ]
      },
      {
        "name": "logs",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "LogsResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/logs",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["GET"]
            }
          }
        ]
      },
       {
        "name": "logs_filename",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "LogsFileNameResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/logs/<filename>",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["GET"]
            }
          }
        ]
      },
       {
        "name": "signup",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "SignUpResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/signup",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["POST"]
            }
          }
        ]
      },
      {
        "name": "patients",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "PatientsResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/patients",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["GET"]
            }
          }
        ]
      },
      {
        "name": "pharmacies",
        "flask": {
          "resourceModule": "health_company_data_api.resources",
          "resourceClass": "PharmaciesResource",
          "strictSlashes": false
        },
        "methods": [
          {
            "path": "/pharmacies",
            "cors": {
              "enable": true,
              "allowHeaders": ["*"],
              "origins": "*",
              "methods": ["GET"]
            }
          }
        ]
      }
    ]
  }
}